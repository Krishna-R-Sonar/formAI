// File: formAI/backend/package.json
{
  "name": "formai",
  "version": "1.0.0",
  "description": "AI-powered form creation and management platform",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "cd frontend && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd frontend && npm install",
    "build": "cd frontend && npm run build"
  },
  "keywords": ["forms", "ai", "react", "nodejs", "mongodb"],
  "author": "FormAI Team",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.2",
    "node-cron": "^4.2.1",
    "papaparse": "^5.5.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3"
  }
}
